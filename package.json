  "dependencies": {
    "electron": "^3.0.4",
    "immer": "^1.8.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "reselect": "^4.0.0"
